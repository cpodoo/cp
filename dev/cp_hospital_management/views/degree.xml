<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<record id="view_doc_degree" model="ir.ui.view">
	    <field name="name">Qualifications</field>
	    <field name="model">degree.data</field>
	    <field name="arch" type="xml">
			<form string = 'Degree'>
				<sheet>
					<group>
						<group>
							<field name="name"/>
						</group>
					</group>
				</sheet>
				<chatter/>
			</form>
		</field>
	</record>

	<record id="view_degree_data_list_view" model="ir.ui.view">
	    <field name="name">Qualifications</field>
	    <field name="model">degree.data</field>
	    <field name="arch" type="xml">
			<list string = 'Degree'>
				<field name="name"/>
			</list>
		</field>
	</record>

	<record id="action_degree_form" model="ir.actions.act_window">
		<field name="name">Qualifications</field>
		<field name="res_model">degree.data</field>
		<field name="view_mode">list,form</field>
	</record>
	<menuitem name="Qualifications" id="menu_degree" parent="menu_config_data" action="action_degree_form" sequence="3"/>
</odoo>